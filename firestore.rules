service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read;
    }

    match /users/{walletId} {
      allow read, create, update;
    }

    match /stones/{gemId} {
      allow read, create, update;
    
    }
    match /pending/{tx} {
      allow read, create, delete;
    }

    match /countries/{country} {
      allow read, create, update;
    }
  }
}



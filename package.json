  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "coverage": "jest --env=jsdom --coverage",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "dev": "npx cypress open"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@storybook/addon-backgrounds": "^3.4.11",
    "@storybook/addon-knobs": "^3.4.11",
    "@storybook/react": "^4.0.0-alpha.24",
    "babel-plugin-import": "^1.9.1",
    "babel-plugin-styled-components": "^1.8.0",
    "cypress": "^3.1.1",
    "cypress-testing-library": "^2.3.1",
    "dom-testing-library": "^3.12.0",
    "dotenv": "^6.0.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.26.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "faker": "^4.1.0",
    "firebase-mock": "^2.2.10",
    "grpc": "^1.16.0",
    "history": "^4.7.2",
    "jest-dom": "^2.1.0",
    "react-test-renderer": "^16.5.2",
    "react-testing-library": "^5.1.0",
    "riteway": "^3.0.1",
    "storybook-addon-jsx": "^5.4.0",
    "truffle-privatekey-provider": "^0.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": "2018"
    },
    "plugins": [
      "eslint-plugin-cypress"
    ],
    "env": {
      "cypress/globals": true,
      "browser": true,
      "jest": true
    }
  }
}
